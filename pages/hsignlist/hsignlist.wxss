/* pages/hsignlist/hsignlist.wxss */
.list_num {
  display: flex;
  justify-content: center;
  color: grey;
  margin-top:5px;
}

.list_top {
  display: flex;
  justify-content: space-between;
  font-size: 12pt;
  margin-left: 15px;
  margin-right: 15px;
  margin-bottom:12px;
  margin-top:10px;
}

.list {
  display: flex;
  justify-content: space-between;
  font-size: 10pt;
  margin-left: 5px;
  margin-right: 5px;
}
.signTime{
  display: flex;
  align-items: center;
  font-size: 8pt;
  color: gainsboro;
}
.sname{
  position: absolute;
  right:10px;
}
.box{
  height: 20px;
  width: 280px;
  margin-top: 5px;
  margin-left: 20rpx;
  margin-right: 20rpx;
  border-radius: 5px; /*圆角弧度 */
  border-color: black; /*边框颜色 */
  background-color: rgb(162, 241, 173);
  padding-left:5px;
  position: relative;
}

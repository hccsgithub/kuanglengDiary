/* pages/signlist/signlist.wxss */
.stopSign {
  margin-left: 15px;
  margin-right: 15px;
  margin-top:15px;
  background-color: #f8c72c;
  color:white
}

.list_num {
  display: flex;
  justify-content: center;
  color: gray;
  margin-top:5px;
  margin-bottom: 5px;
}

.list_top {
  display: flex;
  justify-content: space-between;
  font-size: 12pt;
  margin-left: 15px;
  margin-right: 15px;
}

.list {
  display: flex;
  justify-content: space-between;
  font-size: 10pt;
  margin-left: 5px;
  margin-right: 5px;
}
.signTime{
  display: flex;
  align-items: center;
  font-size: 8pt;
  color: gainsboro;
}
